{
  "expo": {
    "name": "Supfam",
    "slug": "Supfam",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "supfam",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 30
    },
    "assetBundlePatterns": ["**/*"],
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#ffffff",
      "androidMode": "collapse"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mutualflourishing.supfam",
      "buildNumber": "0.0.20",
      "infoPlist": {
        "NSCameraUsageDescription": "Allowing Supfam to use your camera lets you send photos to your fam",
        "NSContactsUsageDescription": "Allowing Supfam to use your contacts lets you find your fam more quickly",
        "NSPhotoLibraryUsageDescription": "Allowing Supfam to use your camera roll lets you send saved photos to your fam"
      }
    },
    "android": {
      "package": "com.mutualflourishing.supfam",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "permissions": [
        "CAMERA",
        "READ_CONTACTS",
        "com.sec.android.provider.badge.permission.READ",
        "com.sec.android.provider.badge.permission.WRITE",
        "READ_PHONE_STATE",
        "READ_INTERNAL_STORAGE",
        "VIBRATE"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/icon-transp-128.png"
      }
    },
    "description": "",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "mutual-flourishing-inc",
            "project": "supfam",
            "authToken": "ff24c5b69e1c43879fdfc246fb4a73ac190956a17a3546708a11b046d0bbac58"
          }
        }
      ]
    }
  }
}
